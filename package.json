{
  "name": "@momei-x/gpm",
  "version": "1.4.0",
  "description": "simple git proxy manager",
  "main": "index.js",
  "module": "index.js",
  "type": "module",
  "bin": {
    "gpm": "dist/index.cjs"
  },
  "files": [
    "dist",
    "package.json",
    "README.MD"
  ],
  "scripts": {
    "test": "tsx tests/simple-test-runner.ts",
    "test:unit": "tsx tests/simple-test-runner.ts",
    "dev": "tsdown --watch",
    "start": "tsx src/index.ts",
    "build": "tsdown",
    "prepublishOnly": "pnpm build",
    "release": "standard-version",
    "pub": "pnpm release && npm publish --access public"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "momei",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.4",
    "standard-version": "^9.5.0",
    "tsdown": "^0.11.9",
    "tsx": "^4.19.3",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4"
  }
}
